{
    "manifest_version": 3,
    "name": "NYC Apartment Hunter",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "contextMenus"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup/popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }